{
  "manifest_version": 3,
  "name": "FreeMedium",
  "version": "2.1",
  "description": "An extension to read Medium posts for free.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.medium.com/*",
        "https://ai.gopubby.com/*",
        "https://ai.plainenglish.io/*",
        "https://andrewzuo.com/*",
        "https://article.darkiee.com/*",
        "https://articles.readytowork.jp/*",
        "https://astropagan.com/*",
        "https://atomic.engineering/*",
        "https://aws.plainenglish.io/*",
        "https://awstip.com/*",
        "https://baos.pub/*",
        "https://bettermarketing.pub/*",
        "https://betterprogramming.pub/*",
        "https://blog.angular.dev/*",
        "https://blog.bitsrc.io/*",
        "https://blog.curiosity.ai/*",
        "https://blog.det.life/*",
        "https://blog.devgenius.io/*",
        "https://blog.devops.dev/*",
        "https://blog.prototypr.io/*",
        "https://blog.ricofritzsche.de/*",
        "https://blog.stackademic.com/*",
        "https://code.likeagirl.io/*",
        "https://deep.sweet.pub/*",
        "https://drlee.io/*",
        "https://ehandbook.com/*",
        "https://fanfare.pub/*",
        "https://faun.pub/*",
        "https://generativeai.pub/*",
        "https://ideaswithwings.org/*",
        "https://infosecwriteups.com/*",
        "https://itnext.io/*",
        "https://javascript.plainenglish.io/*",
        "https://levelup.gitconnected.com/*",
        "https://li.earthonline.us/*",
        "https://medium.astrafy.io/*",
        "https://medium.datadriveninvestor.com/*",
        "https://medium.productcoalition.com/*",
        "https://muddyum.net/*",
        "https://programming.earthonline.us/*",
        "https://pub.aimind.so/*",
        "https://pub.towardsai.net/*",
        "https://publication.osintambition.org/*",
        "https://python.plainenglish.io/*",
        "https://readcultured.com/*",
        "https://towardsdatascience.com/*",
        "https://towardsdev.com/*",
        "https://uxdesign.cc/*",
